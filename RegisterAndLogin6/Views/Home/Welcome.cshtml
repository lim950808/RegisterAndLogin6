@model RegistrationAndLogin7.Models.Account
@{
    ViewBag.Title = "Welcome";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Success</title>
</head>
<body>

    <h2>환영합니다.</h2>
    <hr />
    Welcome @Session["UserId"]
    @if (Request.IsAuthenticated)
    {

        <span>Welcome @Html.Encode(User.Identity.Name) </span>
    }
    <br>
    <a href="@Url.Action("Logout", "Account")">Logout</a>
</body>
</html>